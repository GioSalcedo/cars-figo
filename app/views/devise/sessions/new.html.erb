<div class="car-title">
  <h1>Inicia sesión</h1>
</div>

<div class="ed-user-log-form">
  <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name),
    data: {turbo: false}) do |f| %>
      <%= f.input :email,
                  required: false,
                  autofocus: true,
                  input_html: { autocomplete: "email" } %>
      <%= f.input :password,
                  required: false,
                  input_html: { autocomplete: "current-password" } %>
      <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>

          <div class="header-btn sign-in">
      <%= f.button :submit, "Iniciar Sesión", :class => "header-btn sign-in"  %>


    </div>
      <%= render "devise/shared/links", :class => "header-btn" %>
    </div>



  <% end %>



</div>
