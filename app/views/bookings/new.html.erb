<div class="car-title">
  <h1>Nueva reserva</h1>

</div>
<div class="book-form">
  <div class="row justify-content-center">
    <div class="col-md-6">
      <%= simple_form_for [@vehicle, @booking] do |f| %>
      <%= f.input :s_date, as: :date, order: [:day, :month, :year] %>
      <%= f.input :f_date, as: :date, order: [:day, :month, :year] %>
      <%= f.button :submit , class: "action-btn sign-in" %>
      <% end %>
    </div>
  </div>
</div>
